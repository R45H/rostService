//-
	data

		img: Путь к картинке
		name: имя
		position: должность
		mail: изменяемая часть почты
		tel: телефон

mixin specialist(data)
	.specialist
		//img.specialist__img(src= data.img)
		.specialist__img(
			style="background-image: url('" + data.img + "');"
		)
		.specialist__wrap
			.specialist__name.subtitle= data.name
			.specialist__position= data.position
			a.specialist__mail.main__link(href= "mailto:" + data.mail + "@rostservice.com")
				span.red= data.mail
				| @rostservice.com
			a.specialist__tel.main__link(href= "tel:" + data.telLink)= data.tel